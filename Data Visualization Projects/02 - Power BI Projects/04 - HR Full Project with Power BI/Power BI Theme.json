{
	"name": "Theme Light - BIBB.PRO",
	"$schema": "https://raw.githubusercontent.com/microsoft/powerbi-desktop-samples/main/Report%20Theme%20JSON%20Schema/reportThemeSchema-2.115.json",
	"dataColors": [
		"#4eb473",
		"#f8f9fb",
		"#a3dbd3",
		"#87be5b",
		"#944b40"
	],
	"textClasses": {
		"callout": {
			"fontFace": "Arial",
			"color": "#292E6B"
		},
		"header": {
			"color": "#292E6B",
			"fontFace": "Arial"
		},
		"label": {
			"color": "#292E6B",
			"fontFace": "Arial",
			"fontSize": 9
		},
		"largeTitle": {
			"color": "#292E6B",
			"fontFace": "Georgia",
			"fontSize": 12
		},
		"title": {
			"color": "#292E6B",
			"fontFace": "Arial",
			"fontSize": 9
		}
	},
	"bad": "#ee1c25",
	"neutral": "#949599",
	"good": "#38b64b",
	"maximum": "#00243A",
	"minimum": "#b2d7ed",
	"center": "#007bc4",
	"firstLevelElements": "#292E6B",
	"secondLevelElements": "#292E6B",
	"thirdLevelElements": "#292E6B",
	"fourthLevelElements": "#292E6B",
	"background": "#F6F8F9",
	"secondaryBackground": "#F6F8F9",
	"visualStyles": {
		"*": {
			"*": {
				"background": [
					{
						"color": {
							"solid": {
								"color": "#F6F8F9"
							}
						},
						"transparency": 0
					}
				],
				"border": [
					{
						"color": {
							"solid": {
								"color": "#F6F8F9"
							}
						},
						"radius": 5,
						"show": true,
						"transparency": 0
					}
				],
				"categoryAxis": [
					{
						"showAxisTitle": false
					}
				],
				"dropShadow": [
					{
						"show": true,
						"color": {
							"solid": {
								"color": "#001A29"
							}
						},
						"shadowSpread": 0,
						"shadowBlur": 10,
						"angle": 45,
						"shadowDistance": 10,
						"transparency": 75,
						"position": "Outer",
						"preset": "Custom"
					}
				],
				"legend": [
					{
						"position": "TopCenter",
						"showTitle": false
					}
				],
				"padding": [
					{
						"top": 10
					},
					{
						"bottom": 10
					},
					{
						"left": 10
					},
					{
						"right": 10
					}
				],
				"valueAxis": [
					{
						"showAxisTitle": false
					}
				]
			}
		},
		"actionButton": {
			"*": {
				"outline": [
					{
						"show": false
					}
				]
			}
		},
		"card": {
			"*": {
				"labels": [
					{
						"fontSize": 20
					}
				],
				"padding": [
					{
						"bottom": 0
					},
					{
						"top": 0
					},
					{
						"left": 0
					},
					{
						"right": 0
					}
				]
			}
		},
		"image": {
			"*": {
				"background": [
					{
						"show": false
					}
				],
				"border": [
					{
						"show": false
					}
				],
				"dropShadow": [
					{
						"show": false
					}
				]
			}
		},
		"page": {
			"*": {
				"background": [
					{
						"color": {
							"solid": {
								"color": "#E6ECF0"
							}
						},
						"transparency": 0
					}
				],
				"outspacePane": [
					{
						"backgroundColor": {
							"solid": {
								"color": "#E6ECF0"
							}
						}
					}
				],
				"filterCard": [
					{
						"$id": "Available",
						"backgroundColor": {
							"solid": {
								"color": "#E6ECF0"
							}
						},
						"foregroundColor": {
							"solid": {
								"color": "#292E6B"
							}
						},
						"inputBoxColor": {
							"solid": {
								"color": "#F6F8F9"
							}
						},
						"transparency": 0
					},
					{
						"$id": "Applied",
						"backgroundColor": {
							"solid": {
								"color": "#F6F8F9"
							}
						},
						"foregroundColor": {
							"solid": {
								"color": "#292E6B"
							}
						},
						"inputBoxColor": {
							"solid": {
								"color": "#F6F8F9"
							}
						},
						"transparency": 0
					}
				]
			}
		},
		"shape": {
			"*": {
				"background": [
					{
						"color": {
							"solid": {
								"color": "#F6F8F9"
							}
						},
						"show": true,
						"transparency": 0
					}
				],
				"fill": [
					{
						"show": false
					}
				],
				"outline": [
					{
						"show": false
					}
				],
				"_comments": ""
			}
		},
		"slicer": {
			"*": {
				"background": [
					{
						"show": false
					}
				],
				"dropShadow": [
					{
						"show": false
					}
				],
				"header": [
					{
						"show": true
					}
				],
				"items": [
					{
						"background": {
							"solid": {
								"color": "#E6ECF0"
							}
						}
					}
				]
			}
		},
		"tableEx": {
			"*": {
				"grid": [
					{
						"gridHorizontal": false,
						"rowPadding": 4,
						"outlineColor": {
							"solid": {
								"color": "#4eb473"
							}
						}
					}
				],
				"title": [
					{
						"show": true
					}
				],
				"values": [
					{
						"backColorPrimary": {
							"solid": {
								"color": "#F6F8F9"
							}
						},
						"backColorSecondary": {
							"solid": {
								"color": "#F6F8F9"
							}
						}
					}
				]
			}
		},
		"textbox": {
			"*": {
				"background": [
					{
						"show": false
					}
				],
				"border": [
					{
						"show": false
					}
				],
				"dropShadow": [
					{
						"show": false
					}
				],
				"padding": [
					{
						"bottom": 0
					},
					{
						"left": 0
					},
					{
						"right": 0
					},
					{
						"top": 0
					}
				]
			}
		}
	}
}
